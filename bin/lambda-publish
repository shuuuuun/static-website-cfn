#!/bin/sh

set -eu

ROOT_PATH="$(cd "$(dirname $0)/../"; pwd)"
source "$ROOT_PATH/env"

FUNCTION_NAME="basic_auth"

res=`aws lambda publish-version \
  --region $LAMBDA_REGION \
  --profile $AWS_PROFILE \
  --function-name $FUNCTION_NAME`

echo "$res"
echo "$res" | jq -r ".FunctionArn" > ./lambda_func_arn.txt
